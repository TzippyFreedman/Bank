{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "ConnectionStrings": {
    "BankUserServiceConnectionString": "Server=C1; Database= bankUser; Trusted_Connection = True;",
    "AllowedHosts": "*"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "MSSqlServer",
        "Args": {
          "connectionString": "BankUserServiceConnectionString",
          "tableName": "Log",
          "autoCreateSqlTable": "true"
        }
      }
    ]

  }
}
